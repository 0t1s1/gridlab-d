object inverter {
	name const_pq_inv1;
	parent 1370;
	inverter_type FOUR_QUADRANT;
	use_multipoint_efficiency FALSE;
	four_quadrant_control_mode CONSTANT_PQ;
	generator_status ONLINE;
	inverter_efficiency 0.99;	
	enable_ramp_rates_real false;
	max_ramp_down_real 3.0 kW/s;
	max_ramp_up_real 3.0 kW/s;
	enable_ramp_rates_reactive false;
	max_ramp_down_reactive 1000;
	max_ramp_up_reactive 1000;

	rated_power 300 kVA;
	P_Out 200000;
	Q_Out 100000;
	V_In 1000+0j;
	I_In 1000+0j;

	flags DELTAMODE;
	dynamic_model_mode PI;
	inverter_convergence_criterion 0.001;
	// PI controller parameters
	kpd 0.001;
	kid 0.09;
	kpq 0.001;
	kiq 0.09;
	
	// Droop curve parameters
	inverter_droop_fp true;
	Tfreq_delay 0.01; 
	R_fp 0.04;
	
	inverter_droop_vq false;
	Tvol_delay 0.01; 
	R_vq 0.005;

	// object recorder {
		// property VA_Out.real, VA_Out.imag;
		 // file PV1_pq_output.csv;
		 // flags DELTAMODE;
		 // interval 1;
	// };
	
	object complex_assert {
		flags DELTAMODE;
		target VA_Out;
		object player {
			flags DELTAMODE;
			property value;
			file "../constantQ_PV_VA.csv";
		};
		within "0.1";
	};	
};	
